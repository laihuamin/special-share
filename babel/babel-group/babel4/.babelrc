{
    "presets": [
        [
            "env",
            {
                "modules": false
            }
        ],
        "stage-3"
    ],
    "plugins": [
        "transform-class-properties",
        [
            "transform-runtime",
            {
                "helpers": false,
                "polyfill": false,
                "regenerator": true
            }
        ],
        "transform-es2015-modules-commonjs"
    ]
}